{
  "name": "board-server",
  "version": "1.0.0",
  "description": "An apollo-express server for the whiteboard app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/**",
    "test": "NODE_ENV=test jest --testTimeout=10000 --coverage --watchAll",
    "start": "node src/server.js",
    "server": "nodemon src/server.js",
    "client": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joshdcuneo/board.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "express",
    "react"
  ],
  "author": "Josh Cuneo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdcuneo/board/issues"
  },
  "homepage": "https://github.com/joshdcuneo/board#readme",
  "dependencies": {
    "@hapi/joi": "15.0.2",
    "compression": "1.7.4",
    "concurrently": "^3.6.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.16.3",
    "express-http-context": "1.2.3",
    "helmet": "3.22.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^5.2.7",
    "node-uuid": "1.4.8",
    "passport": "0.4.1",
    "passport-facebook": "3.0.0",
    "passport-google-oauth": "2.0.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "6.0.1",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "jest": "26.0.1",
    "nodemon": "^1.18.3",
    "supertest": "4.0.2"
  }
}
